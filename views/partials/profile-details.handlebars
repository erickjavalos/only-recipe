<div class="row" style="width:100%;justify-content:center;">
    <h1 class="">Recipes</h1>
</div>
    <div class="row recipe-overview" style="width:100%;">
        {{!-- iterate through each recipe --}}
        {{#each recipes}}
            <div id="{{recipe_id}}" class="card card-hover">
                <h4 class="mb-5">{{recipes_name}}</h4>
                {{!-- <img
                    src= 'https://ipfs.io/ipfs/{{images}}'
                    alt="Recipe Image"
                    class="img-fluid"
                /> --}}
                <p class="mt-4">Difficulty: {{difficulty}}</p>
                <p>Servings: {{servings}}</p>
                <p>Total Time: {{totaltime}}</p>
            </div>

        {{/each}}

    </div>
<script src="/js/recipe.js"></script>
